generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model all_news {
  id                String   @id @default(auto()) @map("_id") @db.ObjectId
  Author            String
  Category          String[]
  /// Could not determine type: the field only had null or empty values in the sample set.
  Description       String?
  EditorChoice      Boolean?
  Heading           String
  Language          String?
  Region            String?
  Section           String?
  State             String
  Sub_category      String[]
  Tag               String[]
  Thumbnail         String?
  local_image_url   String?
  x_id              String?
  Title             String
  Image_description String?
  Title_image       String
  Location          String?
  v                 Int      @map("__v")
  createdAt         DateTime @db.Date
  currentDate       DateTime @db.Date
  updatedAt         DateTime @db.Date
}

model draft_news {
  id String @id @default(auto()) @map("_id") @db.ObjectId
}

model youtube_shorts {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  ShortUrl  String
  v         Int      @map("__v")
  createdAt DateTime @db.Date
  updatedAt DateTime @db.Date
}

model ads {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  imageUrl  String?
  link      String?
  heading   String?
  title     String?
  v         Int      @map("__v")
  createdAt DateTime @db.Date
  updatedAt DateTime @db.Date
}
